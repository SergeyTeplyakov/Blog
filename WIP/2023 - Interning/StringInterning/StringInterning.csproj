	  <BuiltInComInteropSupport>true</BuiltInComInteropSupport>-->
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BenchmarkDotNet" Version="0.13.11" />
  </ItemGroup>

</Project>
